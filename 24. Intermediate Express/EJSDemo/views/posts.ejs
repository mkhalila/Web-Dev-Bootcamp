<% include partials/header %>

<h1>The Post Page</h1>
<ul>
<% for (var i = 0; i < posts.length; i++) { %>
  <% var post = posts[i]; %>
  <li>
    <%= post.title %> -
    <strong><%= post.author %></strong>
  </li>
<% } %>

<% posts.forEach (function (post) { %>
  <li>
    <%= post.title %> -
    <strong><%= post.author %></strong>
  </li>
<% }); %>
</ul>

<% include partials/footer %>
